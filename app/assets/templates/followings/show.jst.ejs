<h5> Followers:
<% friendFollowers.forEach(function(friendFollower){ %>
  <%= friendFollower.get("name") + ", " %>
<% }) %>
<% if (numExFollowers > 0 ){ %>
   <%= "and " + numExFollowers + "others "%>
 <% } %>

<% if (!objFollowingtemplate){ %>
	<form id="follow" style="display:inline-block">
  	    <input type="hidden" id="followable_id" value="<%= onObjectID %>">
  		 <input type="submit" value="Follow" class="btn btn-primary btn-xs" >
	</form>
<% } else { %>
	<form id="unfollow" style="display:inline-block">
	<input type="hidden" id="follow_id" value="<%= objFollowingtemplate.id %>">
  	<input type="submit" value="Unfollow" class="btn btn-primary btn-xs">
	</form>
<% } %>
 </h5>